{
	"app": "assoc-proxy",
	"version": "1.0.0",
	"data": {
		"applications": [
			{
				"name": "Notepad++",
				"path": "notepad++ ${path}"
			},
			{
				"name": "VS Code",
				"path": "code ${path}"
			},
			{
				"name": "Opera",
				"path": "opera ${path}"
			},
			{
				"name": "Vim",
				"path": "vim ${path}"
			},
			{
				"name": "Vim Read-Only",
				"path": "vim -R ${path}"
			}
		],
		"associations": {
			"file-extensions": [
				{
					"id": "txt...invalid",
					"applications": [
						0,
						1,
						3,
						4
					]
				},
				{
					"id": "js",
					"applications": [
						1
					]
				}
			],
			"uri-schemes": [
				{
					"id": "http",
					"applications": [
						2
					]
				},
				{
					"id": "https",
					"applications": [
						2
					]
				}
			]
		}
	}
}
