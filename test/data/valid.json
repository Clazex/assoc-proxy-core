{
	"app": "assoc-proxy",
	"version": "1.0.0",
	"data": {
		"applications": [
			{
				"name": "Notepad++",
				"command": "notepad++",
				"args": [
					"${path}"
				]
			},
			{
				"name": "VS Code",
				"command": "code",
				"args": [
					"${path}"
				]
			},
			{
				"name": "Opera",
				"command": "opera",
				"args": [
					"${path}"
				]
			},
			{
				"name": "Vim",
				"command": "vim",
				"args": [
					"${path}"
				]
			},
			{
				"name": "Vim Read-Only",
				"command": "vim",
				"args": [
					"-R",
					"${path}"
				]
			}
		],
		"associations": {
			"file-extensions": [
				{
					"id": "txt",
					"applications": [
						0,
						1,
						3,
						4
					]
				},
				{
					"id": "js",
					"applications": [
						1
					]
				}
			],
			"uri-schemes": [
				{
					"id": "http",
					"applications": [
						2
					]
				},
				{
					"id": "https",
					"applications": [
						2
					]
				}
			]
		}
	}
}
